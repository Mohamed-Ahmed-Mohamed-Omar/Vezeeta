@model IEnumerable<Vezeeta.Models.Contact_us.GetAllContactUsVm>

@{
    ViewData["Title"] = "Contact Us List";
}



<div class="t">

    <h2>قائمة الشكاوه </h2>

    <table class="">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                    الاسم
            </th>
            <th>
                    الهاتف
            </th>
            <th>
                    الرسالة
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.phone)
                </td>>
                <td>
                    @Html.DisplayFor(modelItem => item.Message)
                </td>
            </tr>
        }
    </tbody>
</table>
</div>


